{
  "name": "eetasks",
  "displayName": "eetasks",
  "description": "Earth Engine Task Manager",
  "repository": {
    "type": "git",
    "url": "https://github.com/lopezvoliver/eetasks.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "eetasks.open",
        "title": "EE Tasks: view"
      }
    ],
    "configuration":{
        "title": "Earth Engine Task Manager",
        "properties": {
            "eetasks.limit": {
                "type": ["integer","null"],
                "default": null,
                "description": "Maximum number of tasks to return. Defaults to null (no limit)."
            },
            "eetasks.authMethod":{
                "type": ["string"],
                "default": "earthengine-cli",
                "enum": ["earthengine-cli", "gcloud"],
                "enumDescriptions": ["Use stored credentials in ~/.config/earthengine/credentials",
            "Use gcloud auth"],
                "description": "Method to authenticate to Earth Engine."
            }
        }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "node ./esbuild.js",
    "watch": "node ./esbuild.js --watch",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "NODE_ENV=production node ./esbuild.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@types/vscode-webview": "^1.57.2",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "esbuild": "^0.16.17",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@google/earthengine": "^0.1.371",
    "@vscode/webview-ui-toolkit": "^1.2.2"
  }
}
